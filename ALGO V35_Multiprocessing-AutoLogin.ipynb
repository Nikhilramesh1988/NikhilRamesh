    "global kite\n",
    "global order_id\n",
    "global Game\n",
    "def Auto_Trade_job_MM ():\n",
    "    \n",
    "    ZERODHA_API_KEY='####'\n",
    "    ZERODHA_API_SECRET='####'\n",
    "    ZERODHA_USER_ID='####'\n",
    "    ZERODHA_USER_PWD='####'\n",
    "    ZERODHA_TOTP_KEY='####'\n",
    "\n",
    "    #AutoLogin\n",
    "    driver=uc.Chrome()\n",
    "    driver.get(f'https://kite.trade/connect/login?api_key={ZERODHA_API_KEY}&v=3')\n",
    "    login_id = WebDriverWait(driver, 10).until(\n",
    "        lambda x: x.find_element(by=By.XPATH, value='//*[@id=\"userid\"]'))\n",
    "    login_id.send_keys(ZERODHA_USER_ID)\n",
    "\n",
    "    pwd = WebDriverWait(driver, 10).until(\n",
    "        lambda x: x.find_element(by=By.XPATH, value='//*[@id=\"password\"]'))\n",
    "    pwd.send_keys(ZERODHA_USER_PWD)\n",
    "\n",
    "    submit = WebDriverWait(driver, 10).until(lambda x: x.find_element(by=By.XPATH,value='//*[@id=\"container\"]/div/div/div[2]/form/div[4]/button'))\n",
    "    submit.click()\n",
    "\n",
    "    time.sleep(5)\n",
    "\n",
    "\n",
    "    authkey = pyotp.TOTP(ZERODHA_TOTP_KEY)\n",
    "    totp = WebDriverWait(driver, 10).until(lambda x: x.find_element(by=By.XPATH,value='//*[@id=\"userid\"]'))\n",
    "    totp.send_keys(authkey.now())\n",
    "\n",
    "    continue_btn = WebDriverWait(driver, 10).until(lambda x: x.find_element(by=By.XPATH,value='//*[@id=\"container\"]/div[2]/div/div[2]/form/div[2]/button'))\n",
    "    continue_btn.click()\n",
    "\n",
    "    time.sleep(5)\n",
    "\n",
    "    url = driver.current_url\n",
    "    initial_token = url.split('request_token=')[1]\n",
    "    request_token = initial_token.split('&')[0]\n",
    "    driver.close()\n",
    "    \n",
    "    global kite\n",
    "    kite = KiteConnect(api_key=ZERODHA_API_KEY)\n",
    "    print('Request Token:',request_token)\n",
    "    data = kite.generate_session(request_token, api_secret=ZERODHA_API_SECRET)\n",
    "    kite.set_access_token(data['access_token'])\n",
    "    print('Access Token:',data[\"access_token\"])\n",
    "\n",
    "    #Auto Login End\n",
    "    \n",
    "    Amount=900\n",
    "    Leverage=4\n",
    "    Last_close=kite.quote('NSE:IDBI')['NSE:IDBI']['ohlc']['close']\n",
    "    qty=round((Amount/Last_close))*Leverage\n",
    "\n",
    "    global order_id\n",
    "    global Game\n",
    "    varty=kite.VARIETY_REGULAR\n",
    "    scrip='IDBI'\n",
    "    prd_type=kite.PRODUCT_MIS\n",
    "    exchg=kite.EXCHANGE_NSE\n",
    "    ord_typ=kite.ORDER_TYPE_MARKET\n",
    "    valdty=kite.VALIDITY_DAY\n",
    "    exchange='NSE'\n",
    "\n",
    "    def buyorder():\n",
    "        global order_id\n",
    "        try:\n",
    "            order_id = kite.place_order(variety=varty,\n",
    "                                        tradingsymbol=scrip,\n",
    "                                        exchange=exchg,\n",
    "                                        transaction_type=kite.TRANSACTION_TYPE_BUY,\n",
    "                                        quantity=qty,\n",
    "                                        order_type=ord_typ,\n",
    "                                        product=prd_type,\n",
    "                                        validity=valdty)\n",
    "            logging.info(\"Order placed. ID is: {}\".format(order_id))\n",
    "        except Exception as e:\n",
    "            logging.info(\"Order placement failed: {}\".format(e.message))\n",
    "\n",
    "    def sellorder():\n",
    "        global order_id\n",
    "        try:\n",
    "            order_id = kite.place_order(variety=varty,\n",
    "                                        tradingsymbol=scrip,\n",
    "                                        exchange=exchg,\n",
    "                                        transaction_type=kite.TRANSACTION_TYPE_SELL,\n",
    "                                        quantity=qty,\n",
    "                                        order_type=ord_typ,\n",
    "                                        product=prd_type,\n",
    "                                        validity=valdty)\n",
    "            logging.info(\"Order placed. ID is: {}\".format(order_id))\n",
    "        except Exception as e:\n",
    "            logging.info(\"Order placement failed: {}\".format(e.message))     \n",
    "\n",
    "    #for 1 position scrip\n",
    "\n",
    "    to_dt=datetime.now()\n",
    "    fr_dt=to_dt-timedelta(days=15)\n",
    "    instrument_token=377857\n",
    "\n",
    "    def buyprice ():\n",
    "        BUYPRICE=kite.positions()['net'][0]['buy_price']\n",
    "        return BUYPRICE\n",
    "\n",
    "    def sellprice ():\n",
    "        SELLPRICE=kite.positions()['net'][0]['sell_price']\n",
    "        return SELLPRICE\n",
    "\n",
    "    def pnl ():\n",
    "        PNL=kite.positions()['net'][0]['pnl']\n",
    "        return PNL\n",
    "\n",
    "    def pqty():\n",
    "        P_qty=kite.positions()['net'][0]['quantity']\n",
    "        return  P_qty\n",
    "\n",
    "    def ptime():\n",
    "        # Get the timezone object for London\n",
    "        tz_India = pytz.timezone('Asia/Calcutta')\n",
    "\n",
    "        # Get the current time in London\n",
    "        datetime_India = datetime.now(tz_India)\n",
    "\n",
    "        # Format the time as a string and print it\n",
    "        print(\"India time:\", datetime_India.strftime(\"%H:%M:%S\"))\n",
    "\n",
    "    def Order_Status():\n",
    "        status=kite.orders()[-1]['status']\n",
    "        return status\n",
    "\n",
    "    def LTP_f():\n",
    "        LTP=kite.ltp('NSE:IDBI')['NSE:IDBI']['last_price']\n",
    "        return LTP\n",
    "\n",
    "    def Last_close():\n",
    "        his=kite.historical_data(instrument_token, fr_dt, to_dt, 'minute', continuous=False, oi=False)\n",
    "        df = pd.DataFrame(data=his)\n",
    "        L_close=df['close'].iloc[-1]\n",
    "        return L_close\n",
    "\n",
    "    def Last_day_close():\n",
    "        his=kite.historical_data(instrument_token, fr_dt, to_dt, 'day', continuous=False, oi=False)\n",
    "        df = pd.DataFrame(data=his)\n",
    "        LD_close=df['close'].iloc[-1]\n",
    "        return LD_close\n",
    "\n",
    "    def Last_open():\n",
    "        his=kite.historical_data(instrument_token, fr_dt, to_dt, 'minute', continuous=False, oi=False)\n",
    "        df = pd.DataFrame(data=his)\n",
    "        L_open=df['open'].iloc[-1]\n",
    "        return L_open\n",
    "\n",
    "    def MA():\n",
    "        his=kite.historical_data(instrument_token, fr_dt, to_dt, 'minute', continuous=False, oi=False)\n",
    "        df = pd.DataFrame(data=his)\n",
    "        y=df['close'].to_frame()\n",
    "        MA= y['close'].ewm(span=5).mean()\n",
    "        MA1=y.iloc[-1].values\n",
    "        return MA1\n",
    "\n",
    "    def LTP_MA():\n",
    "        LTP_MA=[]\n",
    "        for i in range(5):\n",
    "            LTP_MA.append(LTP_f())\n",
    "            df2 = pd.DataFrame (LTP_MA)\n",
    "            time.sleep(3)\n",
    "            MA=df2.ewm(span=5).mean()\n",
    "            MA1=MA.iloc[-1].values\n",
    "        return MA1.round(2)\n",
    "\n",
    "    def z_pos():\n",
    "        d=kite.positions()\n",
    "        ops=(d['net'])\n",
    "        if len(ops)==0:\n",
    "            return (0)\n",
    "        else:\n",
    "            return (1)\n",
    "\n",
    "\n",
    "    def bid():\n",
    "        b=kite.quote('NSE:IDBI')\n",
    "        bq=b['NSE:IDBI']['buy_quantity']\n",
    "        sq=b['NSE:IDBI']['sell_quantity']\n",
    "        if bq>sq:\n",
    "            bid='positive'\n",
    "        else:\n",
    "            bid='negetive'\n",
    "        return bid\n",
    "\n",
    "    def predicted_price():\n",
    "\n",
    "        his=kite.historical_data(instrument_token, fr_dt, to_dt, 'minute', continuous=False, oi=False)\n",
    "        df = pd.DataFrame(data=his)\n",
    "        x=df[['open','high','low']]\n",
    "        y=df['close']\n",
    "        reg=linear_model.LinearRegression()\n",
    "        reg.fit(x,y)\n",
    "        Last_row=x.iloc[-1:,[0,1,2]].values\n",
    "        Last_row\n",
    "        pred=reg.predict(Last_row)\n",
    "        return pred\n",
    "\n",
    "#for 1 position scrip End\n",
    "\n",
    "    def Trade_job_MM():\n",
    "        upu=(0.9/100)*Amount\n",
    "        down=-((0.9/100)*Amount)\n",
    "        #begin trade\n",
    "        global Game\n",
    "        for i in range(60):\n",
    "\n",
    "            %store -r LTP_MM_pred2 Game\n",
    "            k1=kite.quote('NSE:IDBI')\n",
    "            today_open,Last_day_close=k1['NSE:IDBI']['ohlc']['open'],k1['NSE:IDBI']['ohlc']['close']\n",
    "            ext=0\n",
    "            if Game==0:\n",
    "                sys.exit('Exited-Game=0')\n",
    "            elif Last_day_close<=today_open<LTP_MM_pred2 and z_pos()==0:\n",
    "                order_id = kite.place_order(variety=varty,\n",
    "                                        tradingsymbol=scrip,\n",
    "                                        exchange=exchg,\n",
    "                                        transaction_type=kite.TRANSACTION_TYPE_BUY,\n",
    "                                        quantity=qty,\n",
    "                                        order_type=ord_typ,\n",
    "                                        product=prd_type,\n",
    "                                        validity=valdty)\n",
    "                print(\"Buy order\")\n",
    "                ext=0\n",
    "                break\n",
    "            else:\n",
    "                print(\"Trade not started\")\n",
    "                time.sleep(23)\n",
    "                ext=1\n",
    "        if ext==1:\n",
    "            Game=0\n",
    "            %store Game\n",
    "            sys.exit('Exited-Trade not started')\n",
    "\n",
    "        else:\n",
    "            pass\n",
    "        starttime=datetime.now()\n",
    "        endtime=starttime+timedelta(hours=6)\n",
    "\n",
    "        #Min\n",
    "        while Game==1 and datetime.now()<endtime:\n",
    "            %store -r LTP_MM_pred2 Game\n",
    "            k=kite.positions()\n",
    "            pt,bp,pn=k['net'][0]['quantity'],k['net'][0]['buy_price'],k['net'][0]['pnl']\n",
    "\n",
    "            if pt==qty and LTP_MM_pred2>=0.10+bp and kite.orders()[-1]['status']=='OPEN':\n",
    "                order_id = kite.modify_order(variety=varty, order_id=order_id,quantity=qty, price=(((LTP_MM_pred2/5).round(2))*5), \n",
    "                                   order_type=kite.ORDER_TYPE_LIMIT,validity=valdty)\n",
    "                print(\"Modify-sellorder Profit\")\n",
    "            elif pt==qty and LTP_MM_pred2<=0.10-bp and kite.orders()[-1]['status']=='OPEN':\n",
    "                order_id = kite.modify_order(variety=varty, order_id=order_id,quantity=qty, price=(((LTP_MM_pred2/5).round(2))*5), \n",
    "                                   order_type=kite.ORDER_TYPE_LIMIT,validity=valdty)\n",
    "                print(\"Modify-sellorder Loss\")\n",
    "\n",
    "            elif pt==qty and LTP_MM_pred2>=0.10+bp and kite.orders()[-1]['status']=='COMPLETE':\n",
    "                order_id = kite.place_order(variety=varty,\n",
    "                                        tradingsymbol=scrip,\n",
    "                                        exchange=exchg,\n",
    "                                        transaction_type=kite.TRANSACTION_TYPE_SELL,\n",
    "                                        quantity=qty,\n",
    "                                        order_type=kite.ORDER_TYPE_LIMIT,\n",
    "                                        price=(((LTP_MM_pred2/5).round(2))*5),\n",
    "                                        product=prd_type,\n",
    "                                        validity=valdty)\n",
    "                print(\"sellorder-profit\")\n",
    "            elif pt==qty and LTP_MM_pred2<bp-0.10 and kite.orders()[-1]['status']=='COMPLETE':\n",
    "                order_id = kite.place_order(variety=varty,\n",
    "                                        tradingsymbol=scrip,\n",
    "                                        exchange=exchg,\n",
    "                                        transaction_type=kite.TRANSACTION_TYPE_SELL,\n",
    "                                        quantity=qty,\n",
    "                                        order_type=kite.ORDER_TYPE_LIMIT,\n",
    "                                        price=(((LTP_MM_pred2/5).round(2))*5),\n",
    "                                        product=prd_type,\n",
    "                                        validity=valdty)\n",
    "                print(\"sellorder-loss\")\n",
    "            elif pt==qty and bp<LTP_MM_pred2<0.10+bp:\n",
    "                print(\"Hold\")\n",
    "            elif pt==0 and pn>upu:\n",
    "                Game=0\n",
    "                %store Game\n",
    "                print('Exited-Profit')\n",
    "                break\n",
    "            elif pt==0 and pn<upu and LTP_MM_pred2 > Last_close() and kite.orders()[-1]['status']=='COMPLETE':\n",
    "                buyorder()\n",
    "                print(\"Buy order\")\n",
    "            elif pt==qty and pn<=down:\n",
    "                sellorder()\n",
    "                Game=0\n",
    "                %store Game\n",
    "                print('Exited-Loss')\n",
    "                break\n",
    "            else:\n",
    "                print(\"No trades placed..\")\n",
    "            time.sleep(23)\n",
    "    schedule.every().day.at(\"11:45:24\").do(Trade_job_MM)"
   ]
